<?xml version='1.0' encoding='utf-8'?>
<widget id="io.nepapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>NepApp</name>
    <description>
        App de encontrar caronas da FEG-Unesp
    </description>
    <author email="silas.vergilio@gmail.com" href="http://cordova.io">
        Silas Studio
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="57" platform="ios" src="res/ios/icon.png" width="57" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="res/icon/android/mdpi/ic_launcher.png" />
    <icon density="hdpi" src="res/icon/android/hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/icon/android/xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="res/icon/android/xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="res/icon/android/xxxhdpi/ic_launcher.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
